/* pages/Set/SwitchAccount/swacc.wxss */
.content {
  margin-top: 36rpx;
  width: 100%;
  /* height: 432rpx; */
  background: #FFFFFF;
}

.cont-list {
  display: flex;
  padding: 33rpx;
  align-items: center;
}

.head-icon {
  width: 84rpx;
  height: 84rpx;
  background: cyan;
  border-radius: 50%;
}

.check-icon {
  
  position: relative;
}

.check-icon::after {
  content: "\2713";
}

.check-icon::after {
  content: "\2713";
  position: absolute;
  bottom: 8rpx;
  right: -2rpx;
  width: 16px;
  height: 16px;
  background-color:#2a9f93;
  color: white;
  font-size: 14px;
  text-align: center;
  line-height: 20px;
  border-radius: 50%;
  z-index: 9999;
}

.cont-txt {
  margin-left: 24rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #000000;
}