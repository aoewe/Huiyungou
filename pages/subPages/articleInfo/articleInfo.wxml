<wxs src="../../../utils/comm.wxs" module="comm"></wxs>
<view class="box">
  <view class="title">{{info.title}}</view>
  <view class="date">{{comm.formatDate(info.create_time)}}</view>
  <van-image custom-class="news-img" src="{{info.banner_url}}" fit="widthFix" lazy-load />
  <rich-text nodes="{{info.content}}"></rich-text>
</view>