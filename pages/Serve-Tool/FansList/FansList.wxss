page {
  background-color: #ffffff;
}

.list {
  padding: 30rpx 0 30rpx 30rpx;
}

.item {
  display: flex;
  align-items: center;
  height: 150rpx;
  position: relative;
}

.item::after {
  content: '';
  position: absolute;
  right: 0;
  bottom: 0;
  background-color: #EEEEEE;
  width: 607rpx;
  height: 2rpx;
}

.item>view {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding-bottom: 6rpx;
  margin-left: 14rpx;
  width: 100%;
  height: 102rpx;
}

.item>view text:last-child {
  color: #999999;
  font-size: 28rpx;
}
